{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' https://your-embedding-site.com"
          },
          {
            "key": "X-Frame-Options",
            "value": "ALLOW-FROM https://your-embedding-site.com"
          }
        ]
      }
    ]
  }